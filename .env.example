# Qlik Configuration
QLIK_API_ENDPOINT=https://your-qlik-server.com/api
QLIK_API_TOKEN=your-qlik-api-token

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=your-account-id

# Step Functions Configuration
STEP_FUNCTION_ARN=arn:aws:states:region:account-id:stateMachine:QlikMonitoringAgent

# Monitoring Configuration
MONITORING_INTERVAL_SECONDS=300
LAG_THRESHOLD_SECONDS=600
STUCK_TASK_THRESHOLD_SECONDS=1800
MAX_RESTART_ATTEMPTS=3

# Logging
LOG_LEVEL=INFO
